{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.rulers": [{
    "column": 80,
    "color": "#4e5461"
  }],
  "i18n-ally.localesPaths": [
    "src/config"
  ],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "pt-BR",
  "jest.autoRun": {
    "watch": false,
    "onSave": "test-file",
    "onStartup": ["all-tests"]
  }
}
