{
  "name": "bookshelf-ui",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "deploy": "build && gh-pages -d dist",
    "dev": "yarn dev:base -- --open",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --fix --no-error-on-unmatched-pattern src/**/*.{js,jsx,ts,tsx}",
    "start": "webpack-dev-server --open",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:report": "xdg-open coverage/lcov-report/index.html",
    "test:staged": "npm test -- --findRelatedTests",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [],
  "author": "Leandro Alves",
  "homepage": "https://leandrokanis.github.io/bookshelf-ui",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/axios": "^0.14.0",
