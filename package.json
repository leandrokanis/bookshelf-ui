    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-localstorage-mock": "^2.4.2",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.0",
    "react-responsive": "^8.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
